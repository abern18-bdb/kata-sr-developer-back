{
  "name": "kata-sr-developer-back",
  "version": "1.0.7",
  "description": "proyecto kata sr developer",
  "private": true,
  "main": "dist/bin/www.js",
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/dist/",
      "/src/config.ts"
    ],
    "moduleNameMapper": {
      "@src(.*)$": "<rootDir>/src/$1",
      "@services(.*)$": "<rootDir>/src/application/services/$1",
      "@test(.*)$": "<rootDir>/test/$1"
    },
    "preset": "ts-jest",
    "verbose": true
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && export DEBUG=bdb:* && node ./dist/bin/www.js",
    "dev": "nodemon --watch src -e ts,js --exec npm start",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coverage:report": "jest --coverage",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "npx eslint '*/**/*.{js,ts}' --fix",
    "prettier:all": "prettier \"**/*.{ts,js,json,html,scss,md}\" --write",
    "prettier": "prettier \"**/*.{ts,js,json,html,scss,md}\" --check",
    "fix:all": "npm run prettier:all && npm run lint:fix",
    "sonar:local": "npm run coverage:report && npm run clean && sonar-scanner",
    "debug": "DEBUG=* node ./dist/bin/www.js",
    "clean": "rm -rf dist .scannerwork"
  },
  "keywords": [
    "typescript",
    "express",
    "js",
    "node-js"
  ],
  "author": "Angelica Bernal",
  "license": "ISC",
  "dependencies": {
    "@npm-bbta/bbog-dig-evo-express-common-lib": "1.0.18",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-openapi-validate": "^0.6.1",
    "express-validator": "^7.2.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "reflect-metadata": "^0.1.13",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/nock": "^11.1.0",
    "@types/node": "^20.5.9",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.3.1",
    "nock": "^13.2.4",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.6.1",
    "sinon": "^13.0.2",
    "sonarqube-scanner": "^2.8.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.1",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3"
  }
}
